/* 1.6 - Tables */
table { width: 100%; border-spacing: 0; }
table tr td { border-top: 1px solid {{ settings.border_color }}; }
table tr td, table tr th { padding: 10px; text-align: left; }
table tr td:first-child, table tr th:first-child { padding-left: 0px; }
table tr td:last-child, table tr th:last-child { padding-right: 0px; }

strong { font-weight: 700; }
em { font-style: italic; }

/* 1.7 - Icons */
.icon-social { float: left; display: block; width: 34px; height: 34px; overflow: hidden; text-indent: -9999px; margin-right: 3px; }
{% if settings.social_icons == "light" %}
.icon-social { background: url('{{ 'icon-social-light.png' | asset_url }}') no-repeat; }
{% endif %}
{% if settings.social_icons == "dark" %}
.icon-social { background: url('{{ 'icon-social-dark8.png' | asset_url }}') no-repeat; }
{% endif %}
.icon-social.twitter { background-position: -46px 0; }
.icon-social.twitter:hover { background-position: -46px -36px; }

.icon-social.facebook { background-position: 0 0; }
.icon-social.facebook:hover { background-position: 0 -36px; }

.icon-social.tumblr { background-position: -92px 0; }
.icon-social.tumblr:hover { background-position: -92px -36px; }

.icon-social.pinterest { background-position: -137px 0; }
.icon-social.pinterest:hover { background-position: -137px -36px; }

.icon-social.youtube { background-position: -224px 0; }
.icon-social.youtube:hover { background-position: -224px -36px; }

.icon-social.insta { background-position: -180px 0; }
.icon-social.insta:hover { background-position: -180px -36px; }

.icon-social.google { background-position: -269px 0; }
.icon-social.google:hover { background-position: -269px -36px; }

.icon-social.polyvore { background-position: -313px 0; }
.icon-social.polyvore:hover { background-position: -313px -36px; }

.icon-facebook { float: left; margin-top: 1px; width: 40px; overflow: hidden; }
.icon-cart { float: left; display: block; width: 20px; height: 16px; margin: 21px 10px 0 0; opacity: 0.{{ settings.icon_cart_opacity }}; filter: alpha(opacity = {{ settings.icon_cart_opacity }}0); }
{% if settings.icon_cart == "none" %}
.icon-cart { display: none; }
{% endif %}
{% if settings.icon_cart == "light" %}
.icon-cart { background: url('{{ 'icon-cart-small.png' | asset_url }}') no-repeat; background-position: 0 -16px; }
{% endif %}
{% if settings.icon_cart == "dark" %}

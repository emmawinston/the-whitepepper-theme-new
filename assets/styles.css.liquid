a:hover { color:{{ settings.link_hover_color }}; }
a:active { outline: none; }
a.btn3, input.btn3 { -webkit-box-shadow:none; -moz-box-shadow:none; box-shadow:none; display:inline-block; height:45px; line-height:28px; border:none; margin:0; padding:0 20px; color:{{ settings.shop_btn3_text_color }}; font-weight:{{ settings.btn_font_weight }}; font-size:{{ settings.btn_font_size }}; text-transform:{{ settings.btn_font_style }}; background:#527bff; width:auto; }

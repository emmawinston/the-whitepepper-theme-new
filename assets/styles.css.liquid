/* PRESS BLOG */

.blogarticle {
	width: 280px;
	height: 370px;
	overflow: hidden;
	margin: 0 20px 40px 20px;
	text-align: center;
	position: relative;
	background-color: #ebebeb;
}

.blogarticle img {
	height: 370px;
	width: 280px;
	min-height: 370px;
	width: auto !important;
	min-width: 280px;
	max-width: 480px;
}

.blogarticle table,
.blogarticle tr,
.blogarticle td {
	height: 280px;
	width: 280px;
	border-top: 0 none;
	padding: 0px;
}

.blogarticle h2.presstitle {
	font-size: 20px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -120px;
	margin-top: -15px;
	opacity: 0;
	width: 240px;
	text-align: center;
	height: 30px;
}

.blogarticle h2.noimagetitle {
	font-size: 14px;
}

.blogarticle blockquote {
	opacity: 1;
	font-size: 18px;
	display: inline-block;
}

.blogarticle .quotewrapper {
	position: relative;
	overflow: hidden;
}

.blogarticle blockquote:before {
	content: '\201c';
	font-size: 48px;
	color: #f5a33f;
	font-family: 'Coustard', serif;
	display: inline-block;
	line-height: 18px;
	position: absolute;
	top: 0;
	left: 0;
	margin-top: 20px;
	margin-left: 20px;
}

.blogarticle blockquote:after {
	content: '\201d';
	font-size: 48px;
	color: #f5a33f;
	font-family: 'Coustard', serif;
	line-height: 18px;
	display: inline-block;
	position: absolute;
	bottom: 0;
	right: 0;
	margin-right: 20px;
}

.blogarticle a h2.presstitle {
	transition: opacity .25s ease-in-out; 
	-moz-transition: opacity .25s ease-in-out; 
	-webkit-transition: opacity .25s ease-in-out;
}

.blogarticle a img {
	transition: opacity .25s ease-in-out; 
	-moz-transition: opacity .25s ease-in-out; 
	-webkit-transition: opacity .25s ease-in-out;
}

.blogarticle a:hover img {
	opacity: 0.2;
}

.blogarticle a:hover h2 {
	opacity: 1;
	z-index: 999;
}

.brick_text img {
	display: none;
}

